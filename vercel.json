{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "dist/public",
  "devCommand": "cd client && npm run dev",
  "cleanUrls": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}